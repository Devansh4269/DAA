#include<iostream>
using namespace std;
int
main ()
{
  int k = 0;
  int i = 0;
  int arr[100];
  int n;

  while (k != 2)
    {
      cout << "Enter number of integers";
      cin >> n;
      for (int i = 0; i<n; i++)
	{
	  cout << "Enter array elements";
	  cin >> arr[i];
	}  
	  cout << "Enter element to be searched";
	  cin >> k;
      for (i = 0; i < n; i++)
	{
	  if (arr[i] == k)
	    {
	      cout << "Key found at index i" << endl;
		  cout<<"number of test cases is\t"<<i+1;
	    }
	  else
	    {
	      cout << "Key not found" << endl;
		  cout<<"number of test cases is\t"<<i+1;
	    }
	}
      k++;
    }
}

